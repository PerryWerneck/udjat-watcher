#include <windows.h>

VS_VERSION_INFO VERSIONINFO
FILEVERSION    @WIN32_FILEVERSION@
PRODUCTVERSION @PACKAGE_MAJOR_VERSION@,@PACKAGE_MINOR_VERSION@,0,0

/*
CREATEPROCESS_MANIFEST_RESOURCE_ID      RT_MANIFEST "app-manifest.xml"
*/

BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "041604B0"
        BEGIN
            VALUE "FileDescription",	"@PACKAGE_DESCRIPTION@\0"
            VALUE "CompanyName",        "Banco do Brasil S/A.\0"
            VALUE "FileVersion",        "@WIN32_VERSION@\0"
            VALUE "LegalCopyright",     "(C) 2020 Banco do Brasil S/A. All Rights Reserved\0"
            VALUE "OriginalFilename",   "@PACKAGE_NAME@.exe\0"
            VALUE "ProductName",        "@PACKAGE_NAME@\0"
            VALUE "ProductVersion",     "@PACKAGE_MAJOR_VERSION@.@PACKAGE_MINOR_VERSION@.0.0\0"
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0416, 0x04B0

    END


END
